{
  "name": "boilerplate-react-webpack",
  "version": "0.2.1",
  "description": "A webpack React starter",
  "repository": "https://github.com/dev-academy-challenges/boilerplate-react-webpack.git",
  "main": "server/index.js",
  "author": "EDA",
  "license": "ISC",
  "scripts": {
    "start": "node server",
    "postinstall": "webpack",
    "webpack": "webpack --watch",
    "dev": "nodemon server",
    "test": "jest --watchAll"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "express": "^4.16.2",
    "http": "^0.0.0",
    "knex": "^0.15.2",
    "pg": "^7.4.3",
    "react": "^16.0",
    "react-dom": "^16.0",
    "react-redux": "^5.0.7",
    "react-thunk": "^1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "sodium": "^2.0.3",
    "superagent": "^3.8.3",
    "webpack": "^3.0"
  },
  "devDependencies": {
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.1.4",
    "nodemon": "^1.18.4",
    "sqlite3": "^4.0.2"
  }
}
